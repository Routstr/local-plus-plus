{
    "root": true,
    "extends": [
        "@react-native"
    ],
    "plugins": [
        "unused-imports",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": "off",
        "no-unused-vars": "off",
        "unused-imports/no-unused-imports": "error",
        "unused-imports/no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "varsIgnorePattern": "^_",
                "args": "after-used",
                "argsIgnorePattern": "^_"
            }
        ]
    }
}